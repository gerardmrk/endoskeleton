{
  "name": "endoskeleton",
  "version": "0.2.7",
  "description": "backbone for all my isomorphic side hobbies",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "start:dev": "node_modules/.bin/nodemon --config .config/nodemon.json index.js",
    "build:client": "NODE_ENV=production node_modules/.bin/webpack --config .config/webpack.client.config.babel.js --env.production --profile --bail",
    "build:renderer": "NODE_ENV=production node_modules/.bin/webpack --config .config/webpack.renderer.config.babel.js --env.production --profile --bail",
    "build:renderer:watch": "NODE_ENV=development node_modules/.bin/webpack --config .config/webpack.renderer.config.babel.js --watch --env.production --profile --bail",
    "hotreload": "NODE_ENV=development node .config/webpack.hotreload.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerardmrk/endoskeleton.git"
  },
  "keywords": [
    "react",
    "webpack",
    "redux",
    "dankmemes"
  ],
  "author": "gerardmrk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gerardmrk/endoskeleton/issues"
  },
  "homepage": "https://github.com/gerardmrk/endoskeleton#readme",
